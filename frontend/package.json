{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "concurrently 'next dev' 'pnpm prettier-watch' 'pnpm lint-watch'",
    "lint": "pnpm prettier && eslint src/**/*.{js,ts,jsx,tsx} --fix",
    "lint-watch": "onchange 'src/**/*.{js,ts,jsx,tsx}' -- eslint {{changed}} --fix",
    "prettier": "prettier --write src/**/*.{js,ts,jsx,tsx}",
    "prettier-watch": "onchange 'src/**/*.{js,ts,jsx,tsx}' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@radix-ui/react-form": "0.0.3",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-switch": "1.0.3",
    "axios": "1.5.1",
    "next": "13.5.4",
    "react": "18",
    "react-dom": "18",
    "react-hook-form": "7.47.0",
    "react-query": "3.39.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "autoprefixer": "10",
    "clsx": "2.0.0",
    "concurrently": "8.2.1",
    "critters": "0.0.20",
    "eslint": "8",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-pii": "1.0.2",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-xss": "0.1.12",
    "onchange": "7.1.0",
    "postcss": "8",
    "prettier": "3.0.3",
    "tailwind-merge": "1.14.0",
    "tailwindcss": "3",
    "tailwindcss-radix": "2.8.0",
    "typescript": "5",
    "windstitch": "0.3.0"
  }
}