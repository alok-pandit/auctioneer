    "postcss": "^8.4.31",
    "prettier": "3.1.0",
    "tailwind-merge": "2.0.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-radix": "2.8.0",
    "typescript": "^5.3.2",
    "windstitch": "0.3.0"
