{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "concurrently 'next dev' 'yarn run prettier-watch' 'yarn run lint-watch'",
    "lint": "yarn prettier && eslint src/**/*.{js,ts,jsx,tsx} --fix",
    "lint-watch": "onchange 'src/**/*.{js,ts,jsx,tsx}' -- eslint {{changed}} --fix",
    "prettier": "prettier --write src/**/*.{js,ts,jsx,tsx}",
    "prettier-watch": "onchange 'src/**/*.{js,ts,jsx,tsx}' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "axios": "1.5.1",
    "immer": "10.0.2",
    "jotai": "2.4.3",
    "jotai-immer": "0.2.0",
    "next": "13.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.2",
    "react-query": "3.39.3",
    "tailwindcss-radix": "2.8.0"
  },
  "devDependencies": {
    "@types/node": "20.7.2",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "autoprefixer": "10.4.16",
    "clsx": "2.0.0",
    "concurrently": "8.2.1",
    "critters": "0.0.20",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-pii": "1.0.2",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-xss": "0.1.12",
    "onchange": "7.1.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "windstitch": "0.3.0"
  }
}